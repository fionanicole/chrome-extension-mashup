{
  "manifest_version": 2,
  "name": "growgarden",
  "version": "1",
  "description": "This extensions alters the look and content of a google images page.",
  "content_scripts": [{
      "matches": ["<all_urls>"],
      "all_frames": true,
      "js": ["growgarden.js"]
  }],
  "web_accessible_resources": [
    "images/cornflower.jpg"
  ]
  }
